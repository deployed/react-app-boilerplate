{
  "name": "deployed-react-app-boilerplate",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "yarn": ">=1.7.0",
    "node": ">=8.10"
  },
  "scripts": {
    "check:engines": "check-node-version --package",
    "postinstall": "yarn check:engines",
    "prebuild": "yarn check:engines",
    "start": "razzle start",
    "build": "razzle build",
    "start:prod": "NODE_ENV=production node build/server.js",
    "lint": "cross-env NODE_ENV=development yarn lint:js && yarn lint:css",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:stylelint": "stylelint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:js": "yarn lint:eslint . ",
    "lint:css": "yarn lint:stylelint .",
    "lint:staged": "lint-staged",
    "pretest": "yarn check:engines && yarn test:clean",
    "test:clean": "rimraf ./reports",
    "test": "cross-env NODE_ENV=test jest --coverage --config internals/testing/jest.config.js",
    "test:watch": "cross-env NODE_ENV=test jest --config internals/testing/jest.config.js --watchAll",
    "test:ci": "yarn test --ci --testResultsProcessor='jest-junit' --coverageReporters='html'"
  },
  "lint-staged": {
    "*.js": [
      "lint:eslint",
      "lint:stylelint"
    ]
  },
  "pre-commit": "lint:staged",
  "jest-junit": {
    "output": "./reports/test-results.xml"
  },
  "dependencies": {
    "@svgr/webpack": "^2.1.0",
    "apollo-cache-inmemory": "^1.2.5",
    "apollo-client": "^2.3.5",
    "apollo-link-http": "^1.5.4",
    "babel-plugin-preval": "^3.0.0",
    "babel-plugin-react-intl": "^2.4.0",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "check-node-version": "^3.2.0",
    "circular-dependency-plugin": "^5.0.2",
    "compression": "^1.7.2",
    "cross-env": "^5.2.0",
    "exports-loader": "^0.7.0",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "image-webpack-loader": "^4.3.1",
    "intl": "^1.2.5",
    "jest-junit": "^5.1.0",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "mobx-react-router": "^4.0.4",
    "raven": "^2.6.3",
    "raven-js": "^3.26.3",
    "razzle": "^2.2.0",
    "react": "^16.4.1",
    "react-apollo": "^2.1.9",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.4.0",
    "react-loadable": "^5.4.0",
    "react-router-dom": "^4.3.1",
    "sanitize.css": "^6.0.0",
    "serialize-javascript": "^1.5.0",
    "styled-components": "^3.3.3",
    "universal-cookie": "^2.2.0",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "apollo-link-schema": "^1.1.0",
    "enzyme": "^3.3.0",
    "enzyme-react-adapter-future": "^1.1.3",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.1.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-react": "^7.10.0",
    "graphql-tools": "^3.0.5",
    "jest-styled-components": "^5.0.1",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.1"
  }
}
